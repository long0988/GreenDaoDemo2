        <!--&lt;!&ndash; 进入和退出的动画 &ndash;&gt;-->
        <item name="android:windowAnimationStyle">@style/view_slide_anim</item>

        <!-- 点外边可以消失  -->
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>

    //放大
    <style name="view_scale_anim" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/view_dialog_scale_in</item>
